## Init settings.
# Parameter list to optimize.
param_list=( TrendMethodAction )
# Disable all optimization settings.
input_set .*,F 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Set lot size.
input_set LotSize 0.01

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test TrendMethodAction.
input_set TrendMethodAction,F 1
input_set TrendMethodAction,1 0
input_set TrendMethodAction,2 1
input_set TrendMethodAction,3 255

