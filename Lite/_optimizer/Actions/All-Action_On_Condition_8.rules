## Init settings.
# Parameter list to optimize.
param_list=( Market_Condition_8 Action_On_Condition_8 )
# Disable all optimization settings.
input_set .*,F 0
# Double check that account conditions are enabled.
input_set Account_Conditions_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Market_Condition_8.
input_set Market_Condition_8,F 1
input_set Market_Condition_8,1 0
input_set Market_Condition_8,2 1
input_set Market_Condition_8,3 14
# Test Action_On_Condition_8.
input_set Action_On_Condition_8,F 1
input_set Action_On_Condition_8,1 0
input_set Action_On_Condition_8,2 1
input_set Action_On_Condition_8,3 10

