## Init settings.
# Parameter list to optimize.
param_list=( Market_Condition_6 )
# Disable all optimization settings.
input_set .*,F 0
# Double check that account conditions are enabled.
input_set Account_Conditions_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Market_Condition_6.
input_set Market_Condition_6,F 1
input_set Market_Condition_6,1 0
input_set Market_Condition_6,2 $(( $(input_get Market_Condition_6) ))
input_set Market_Condition_6,3 $(( $(input_get Market_Condition_6) ))

