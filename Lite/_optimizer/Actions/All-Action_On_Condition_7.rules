## Init settings.
# Parameter list to optimize.
param_list=( Market_Condition_7 Action_On_Condition_7 )
# Disable all optimization settings.
input_set .*,F 0
# Double check that account conditions are enabled.
input_set Account_Conditions_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Market_Condition_7.
input_set Market_Condition_7,F 1
input_set Market_Condition_7,1 0
input_set Market_Condition_7,2 1
input_set Market_Condition_7,3 14
# Test Action_On_Condition_7.
input_set Action_On_Condition_7,F 1
input_set Action_On_Condition_7,1 0
input_set Action_On_Condition_7,2 1
input_set Action_On_Condition_7,3 10

