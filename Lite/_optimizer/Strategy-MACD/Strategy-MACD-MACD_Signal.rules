## Init settings.
# Parameter list to optimize.
param_list=( MACD_Signal_Period )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^MACD.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test MACD_Signal_Period.
input_set ^MACD_Signal_Period,F 1
input_set ^MACD_Signal_Period,1 $(( $(input_get MACD_Signal_Period) > 2 ? $(( $(input_get MACD_Signal_Period) - 2)) : 0 ))
input_set ^MACD_Signal_Period,2 1
input_set ^MACD_Signal_Period,3 $(( $(input_get MACD_Signal_Period) + 2 ))

