## Init settings.
# Parameter list to optimize.
param_list=( Bands15_OpenMethod )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Set lot size.
input_set LotSize 0.01

## Enable Bands strategies.
input_set Bands15.*_Active 1

## Optimization settings.
# Set Bands OpenMethod.
input_set Bands15_OpenMethod,F 1
input_set Bands15_OpenMethod,1 0
input_set Bands15_OpenMethod,2 1
input_set Bands15_OpenMethod,3 255

