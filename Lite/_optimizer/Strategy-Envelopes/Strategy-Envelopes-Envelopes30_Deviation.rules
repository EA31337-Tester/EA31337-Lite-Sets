## Init settings.
# Parameter list to optimize.
param_list=( Envelopes30_Deviation )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^Envelopes30_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test Envelopes30_Deviation.
input_set ^Envelopes30_Deviation,F 1
input_set ^Envelopes30_Deviation,1 $(( $(input_get Envelopes30_Deviation) > 0.2 ? $(input_get Envelopes30_Deviation) - 0.2 : 0.0 ))
input_set ^Envelopes30_Deviation,2 0.1
input_set ^Envelopes30_Deviation,3 $(( $(input_get Envelopes30_Deviation) + 0.2 ))

