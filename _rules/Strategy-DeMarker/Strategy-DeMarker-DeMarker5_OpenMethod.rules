## Init settings.
# Parameter list to optimize.
param_list=( DeMarker5_OpenMethod )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^DeMarker5_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test DeMarker5_OpenMethod.
input_set ^DeMarker5_OpenMethod,F 1
input_set ^DeMarker5_OpenMethod,1 0
input_set ^DeMarker5_OpenMethod,2 3
input_set ^DeMarker5_OpenMethod,3 31

