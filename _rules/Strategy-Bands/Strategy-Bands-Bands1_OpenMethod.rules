## Init settings.
# Parameter list to optimize.
strategy=Bands1
param=${strategy}_OpenMethod
param_list=( $param )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^Bands1_Active 1

## Optimization settings.
# Test Bands OpenMethod.
input_set ^$param,F 1
input_set ^$param,1 0
input_set ^$param,2 7
input_set ^$param,3 255

