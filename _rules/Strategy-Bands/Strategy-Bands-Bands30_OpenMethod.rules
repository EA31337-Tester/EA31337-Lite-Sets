## Init settings.
# Parameter list to optimize.
param_list=( Bands30_OpenMethod )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
input_set .*_Active 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^Bands30_Active 1

## Optimization settings.
# Test Bands OpenMethod.
input_set ^Bands30_OpenMethod,F 1
input_set ^Bands30_OpenMethod,1 0
input_set ^Bands30_OpenMethod,2 7
input_set ^Bands30_OpenMethod,3 255

