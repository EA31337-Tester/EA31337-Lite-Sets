## Init settings.
# Backtest settings.
ini_set_ea maxdrawdown_enable 0
# Parameter list to optimize.
param=Bands_TrailingStopMethod
param_list=( $param )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Set lot size.
input_set LotSize 0.01
# Reset open methods.
input_set ^Bands.*_OpenMethod 0
# Enable Bands strategies.
input_set ^Bands.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test $param.
input_set ^$param,F 1
input_set ^$param,1 0
input_set ^$param,2 2
input_set ^$param,3 27

