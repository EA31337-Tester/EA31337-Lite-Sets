## Init settings.
# Parameter list to optimize.
strategy=Bands5
param=${strategy}_OpenMethod
param_list=( $param )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Enable strategy.
input_set ^Bands5_Active 1

## Optimization rules.
ea_optimize_param_set ${param} 0 255 7

