## Init settings.
# Parameter list to optimize.
param_list=( MA1_OpenMethod )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
input_set .*_Active 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
input_set LotSize 0.01
# Enable strategy.
input_set ^MA1_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test MA1_OpenMethod.
input_set ^MA1_OpenMethod,F 1
input_set ^MA1_OpenMethod,1 0
input_set ^MA1_OpenMethod,2 7
input_set ^MA1_OpenMethod,3 127

