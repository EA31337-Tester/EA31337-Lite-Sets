## Init settings.
# Parameter list to optimize.
param_list=( MA1_SignalMethod )
## Backtest settings.
ini_set ^Period M1
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Enable strategy.
input_set ^MA1_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test MA1_SignalMethod.
input_set ^MA1_SignalMethod,F 1
input_set ^MA1_SignalMethod,1 0
input_set ^MA1_SignalMethod,2 7
input_set ^MA1_SignalMethod,3 127

