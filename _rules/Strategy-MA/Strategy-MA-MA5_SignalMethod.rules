## Init settings.
# Parameter list to optimize.
param_list=( MA5_SignalMethod )
## Backtest settings.
ini_set ^TestPeriod M5
## EA settings.
ea_orders_close_all_at 22
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
ea_strategies_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Disable boosting.
ea_boost_enabled 0
# Set lot size.
ea_set_lotsize 0.01
# Enable strategy.
input_set ^MA5_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test MA5_SignalMethod.
input_set ^MA5_SignalMethod,F 1
input_set ^MA5_SignalMethod,1 0
input_set ^MA5_SignalMethod,2 7
input_set ^MA5_SignalMethod,3 127

