## Init settings.
strategy=Envelopes
# Parameter list to optimize.
param_list=( ${strategy}_TrailingStopMethod )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Set lot size.
input_set LotSize 0.01
# Reset open methods.
input_set ^${strategy}.*_OpenMethod 0
# Enable strategy.
input_set ^${strategy}.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test $strategy_TrailingStopMethod.
input_set ^${strategy}_TrailingStopMethod,F 1
input_set ^${strategy}_TrailingStopMethod,1 0
input_set ^${strategy}_TrailingStopMethod,2 1
input_set ^${strategy}_TrailingStopMethod,3 27

