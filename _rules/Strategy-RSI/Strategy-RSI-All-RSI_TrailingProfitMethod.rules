## Init settings.
# Parameter list to optimize.
param_list=( RSI_TrailingProfitMethod )
# Disable all optimization settings.
ea_optimize_all_set 0
# Disable all active strategies.
input_set .*_Active 0
# Disable boosting.
ea_boost_enabled 0
# Disable account conditions.
ea_actions_enabled 0
# Set lot size.
input_set LotSize 0.01
# Reset open methods.
input_set ^RSI.*_OpenMethod 0
# Enable strategy.
input_set ^RSI.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test RSI_TrailingProfitMethod.
input_set ^RSI_TrailingProfitMethod,F 1
input_set ^RSI_TrailingProfitMethod,1 0
input_set ^RSI_TrailingProfitMethod,2 1
input_set ^RSI_TrailingProfitMethod,3 27

