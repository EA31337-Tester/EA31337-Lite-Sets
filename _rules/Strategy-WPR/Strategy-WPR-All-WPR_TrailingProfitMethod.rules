## Init settings.
# Parameter list to optimize.
param_list=( WPR_TrailingProfitMethod )
# Disable all optimization settings.
input_set .*,F 0
# Disable all active strategies.
input_set .*_Active 0
# Disable boosting.
input_set Boosting_Enabled 0
# Disable account conditions.
input_set Account_Conditions_Active 0
# Set lot size.
input_set LotSize 0.01
# Reset open methods.
input_set ^WPR.*_OpenMethod 0
# Enable strategy.
input_set ^WPR.*_Active 1

## Optimization settings (F-On, 1-Min, 2-Step, 3-Max).
# Test WPR_TrailingProfitMethod.
input_set ^WPR_TrailingProfitMethod,F 1
input_set ^WPR_TrailingProfitMethod,1 0
input_set ^WPR_TrailingProfitMethod,2 1
input_set ^WPR_TrailingProfitMethod,3 27

